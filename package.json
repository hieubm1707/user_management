{
  "name": "voucher-api",
  "version": "0.0.0",
  "description": "a rest api for voucher service",
  "main": "./dist/server.js",
  "scripts": {
    "start": "node ./dist/server.js",
    "start:dev": "cross-env NODE_ENV=development tsc-watch -p ./tsconfig.build.json --onSuccess \"run-s copy-assets start\"",
    "start:stg": "NODE_ENV=staging tsc-watch -p ./tsconfig.build.json --onSuccess \"run-s copy-assets start\"",
    "start:prod": "NODE_ENV=production tsc-watch -p ./tsconfig.build.json --onSuccess \"run-s copy-assets start\"",
    "build": "run-s clean tsc:build copy-assets",
    "tsc:build": "tsc -p ./tsconfig.build.json",
    "clean": "rimraf ./dist",
    "copy-assets": "cpx \"src/**/*.{json,yml,yaml,md,html,css,scss,less,svg,png,jpg,jpeg,gif,ico}\" dist",
    "test": "jest -c jest.config.ts",
    "test:e2e": "jest -c jest-e2e.config.ts --runInBand",
    "docker:start:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up --build",
    "docker:start:test": "docker-compose -f docker-compose.test.yml up --build",
    "docker:start:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up -d --build",
    "db:create": "sequelize db:create",
    "db:drop": "sequelize db:drop",
    "db:seed": "sequelize db:seed:all",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "migrations:create": "sequelize migration:generate --name",
    "seeders:create": "sequelize seed:generate --name",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts,tsx}\" && prettier --check --ignore-unknown .",
    "lint:fix": "tsc --noEmit && eslint \"**/*.{js,ts,tsx}\" --fix && prettier --write --ignore-unknown ."
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "bash -c tsc --noEmit",
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,html,yml,yaml,css}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@adminjs/design-system": "^3.1.8",
    "@adminjs/express": "4.0.1",
    "@adminjs/sequelize": "^3.0.0",
    "@hapi/joi": "^15.1.1",
    "@ssnxd/socketio-jwt": "^4.5.3",
    "@tiptap/core": "^2.12.0",
    "@tiptap/pm": "^2.12.0",
    "@tiptap/starter-kit": "^2.12.0",
    "@types/lodash": "^4.17.13",
    "adminjs": "^7.8.16",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "celebrate": "^10.1.0",
    "compression": "^1.7.4",
    "connect-session-sequelize": "^7.1.7",
    "convict": "^5.2.1",
    "cors": "^2.8.5",
    "dotenv": "^8.6.0",
    "express": "^5.1.0",
    "express-formidable": "^1.2.0",
    "express-jwt": "^6.1.2",
    "express-promise-router": "^3.0.3",
    "express-rate-limit": "^5.5.1",
    "express-session": "^1.18.1",
    "express-status-monitor": "^1.3.4",
    "helmet": "^4.6.0",
    "http-errors": "^1.8.1",
    "i18next": "^19.9.2",
    "i18next-http-middleware": "^3.6.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "node-schedule": "^1.3.3",
    "nodemailer": "^6.9.15",
    "path-to-regexp": "^8.2.0",
    "pg": "^8.13.0",
    "pg-hstore": "^2.3.4",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.1.14",
    "sequelize": "^6.37.4",
    "sequelize-cli": "^5.5.1",
    "sequelize-typescript": "^2.1.6",
    "serve-favicon": "^2.5.0",
    "socket.io": "^2.5.1",
    "socket.io-client": "^2.3.0",
    "typedi": "^0.10.0",
    "uuid": "^11.1.0",
    "winston": "^3.15.0"
